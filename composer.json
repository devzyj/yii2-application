{
    "name": "devzyj/yii2-application",
    "description": "Web Application for Yii2",
    "keywords": ["yii2", "application"],
    "type": "project",
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=5.5.0",
        "yiisoft/yii2": "~2.0.6",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "yiisoft/yii2-swiftmailer": "~2.0.0 || ~2.1.0",
        "yiisoft/yii2-httpclient": "~2.0.0",
        "devzyj/yii2-rest": "~1.0.0",
        "devzyj/yii2-attribute-cache-behavior": "~1.0.0",
        "devzyj/yii2-oauth2-server": "~1.0.0",
        "dmstr/yii2-adminlte-asset": "^2.1",
        "lcobucci/jwt": "~3.2.0",
        "league/oauth2-server": "^6.1",
        "bshaffer/oauth2-server-php": "^1.10",
        "filsh/yii2-oauth2-server": "^2.0"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.0.0",
